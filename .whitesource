{
  "scanSettings": {
    "configMode": "AUTO",
    "enableLicenseViolations": true,
    "baseBranches": []
  },
  "checkRunSettings": {
    "vulnerableCheckRunConclusionLevel": "failure",
    "displayMode": "diff",
    "useMendCheckNames": true
  },
  "issueSettings": {
    "minSeverityLevel": "LOW",
    "issueType": "DEPENDENCY"
  },
  "remediateSettings": {
    "workflowRules": {
      "enabled": true
    }
  },
  "hostRules": [
    {
      "matchHost": "https://pkgs.dev.azure.com/galgernitzky/_packaging/galgernitzky/maven/v1",
      "hostType": "maven",
      "userName": "galgernitzky",
      "encrypted": {
        "password": ""
      },
      "envVariablesMapping": {
        "userName": "AZURE_USERNAME",
        "password": "AZURE_PASSWORD"
      }
    }
  ]
}
